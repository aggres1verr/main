v1 = int(input("Введите скорость первого автомобиля: "))
v2 = int(input("Введите скорость второго автомобиля: "))
t = 0.5
distance_travelled = (v1-v2)*t
def temp(a):
    if v1>v2:
        return distance_travelled
    else:
        v1<v2
    return "Ошибка: по условию скорость первого автомобиля превышает скорость второго автомобиля"
print(f"Расстояние между автомобили после 30 минут езды составит {distance_travelled}")
temperature = float(input("Введите температуру по шкале Цельсия: "))
temp_fahrenheit = temperature*1.8+32
temp_kelvin = temperature+273.15
print(f"Значение температуры по шкале Фаренгейта {temp_fahrenheit}")
print(f"Значение температуры по шкале Кельвина {temp_kelvin}")
fahrenheit = float(input("Введите температуру в градусах по Фаренгейту: "))
celsius = (fahrenheit-32)*5/9
print(f"Температура по Цельсию {celsius:.2f}")
a = float(input("Введите первое число: "))
b = float(input("Введите второе число: "))
sum = a+b
raz = a-b
umn = a*b
chast = a/b
if b!=0:
    chast = a/b
else:
    chast = "Деление на 0 невозможно"
avg_arith = (a+b)/2
print(f"{a:.0f}+{b:.0f}={sum:.0f} {a:.0f}-{b:.0f}={raz:.0f} {a:.0f}*{b:.0f}={umn:.0f} {a:.0f}/{b:.0f}={chast} ({a:.0f}+{b:.0f})/2 = {avg_arith:.1f}")